generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model employees {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  email        String
  name         String
  phone_number String
}
